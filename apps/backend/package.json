{
  "name": "@dibea/backend",
  "version": "1.0.0",
  "description": "DIBEA Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "dev:n8n": "nodemon src/n8n-server.ts",
    "dev:demo": "nodemon src/demo-server.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:n8n": "node dist/n8n-server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:migrate": "prisma migrate dev --schema=./prisma/schema-postgres.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema=./prisma/schema-postgres.prisma",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio --schema=./prisma/schema-postgres.prisma",
    "db:generate": "prisma generate --schema=./prisma/schema-postgres.prisma",
    "db:reset": "prisma migrate reset --schema=./prisma/schema-postgres.prisma --force",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.896.0",
    "@aws-sdk/s3-request-presigner": "^3.899.0",
    "@elastic/elasticsearch": "^9.1.1",
    "@google-cloud/vision": "^5.3.3",
    "@prisma/client": "^5.22.0",
    "@supabase/supabase-js": "^2.58.0",
    "@types/bcrypt": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^0.3.34",
    "mammoth": "^1.11.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "neo4j-driver": "^5.28.2",
    "node-cron": "^3.0.3",
    "openai": "^5.23.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "prisma": "^5.22.0",
    "qrcode": "^1.5.3",
    "redis": "^4.6.12",
    "sharp": "^0.33.5",
    "tesseract.js": "^6.0.1",
    "uuid": "^8.3.2",
    "winston": "^3.11.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/pdf-parse": "^1.1.5",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
