# DIBEA Production Environment Variables
# Copy this file to .env.prod and update with secure values

# Database
DATABASE_URL=postgresql://dibea_user:CHANGE_ME_SECURE_PASSWORD@localhost:5432/dibea_prod
POSTGRES_PASSWORD=CHANGE_ME_SECURE_PASSWORD

# JWT
JWT_SECRET=CHANGE_ME_VERY_SECURE_JWT_SECRET_KEY_2024

# n8n
N8N_USER=admin
N8N_PASSWORD=CHANGE_ME_SECURE_N8N_PASSWORD
N8N_HOST=n8n.yourdomain.com
N8N_WEBHOOK_URL=https://n8n.yourdomain.com
N8N_ENCRYPTION_KEY=CHANGE_ME_VERY_SECURE_N8N_ENCRYPTION_KEY

# Supabase (local PostgreSQL)
SUPABASE_SERVICE_KEY=CHANGE_ME_SECURE_SERVICE_KEY

# Frontend
FRONTEND_API_URL=https://api.yourdomain.com/api/v1

# Redis
REDIS_URL=redis://localhost:6379

# MinIO
MINIO_ROOT_USER=dibea_minio_prod
MINIO_ROOT_PASSWORD=CHANGE_ME_SECURE_MINIO_PASSWORD

# Neo4j
NEO4J_PASSWORD=CHANGE_ME_SECURE_NEO4J_PASSWORD

# Environment
NODE_ENV=production
